<?xml version="1.0" encoding="UTF-8"?>
<beans>

    <bean id="userDao" class="binary.wz.spring.beans.beanfactory.dao.UserDao"/>

    <bean id="userService" class="binary.wz.spring.beans.beanfactory.service.UserService">
        <property name="userId" value="10001"/>
        <property name="company" value="腾讯"/>
